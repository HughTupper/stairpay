{
  "name": "@stairpay/database",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:migration": "supabase migration new",
    "db:types": "mkdir -p types && supabase gen types typescript --local > types/database.ts",
    "db:types:remote": "mkdir -p types && supabase gen types typescript --project-id rnsfmlijruywbaxldpvk > types/database.ts",
    "db:seed": "tsx --env-file=.env.local seed.ts",
    "build": "mkdir -p types && touch types/database.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "@t3-oss/env-core": "^0.13.8",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^20",
    "dotenv": "^17.2.3",
    "supabase": "^1.200.3",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
